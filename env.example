# Supabase Configuration
EXPO_PUBLIC_SUPABASE_URL=your_supabase_url_here
EXPO_PUBLIC_SUPABASE_ANON_KEY=your_supabase_anon_key_here

# Optional: For development only
EXPO_PUBLIC_APP_ENV=development

# API Configuration
# For development with mobile app, use your machine's IP address (not localhost)
# Find your IP with: ipconfig (Windows) or ifconfig (Mac/Linux)
# Example: EXPO_PUBLIC_API_URL=http://YOUR_IP_ADDRESS:3000
# For production, use: EXPO_PUBLIC_API_URL=https://icumbi.com
EXPO_PUBLIC_API_URL=http://localhost:3000

# Optional: Analytics and monitoring
EXPO_PUBLIC_SENTRY_DSN=your_sentry_dsn_here
EXPO_PUBLIC_ANALYTICS_ID=your_analytics_id_here 
